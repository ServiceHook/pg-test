{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/initiate", "dest": "api/initiate.js" },
    { "src": "/api/callback", "dest": "api/callback.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
